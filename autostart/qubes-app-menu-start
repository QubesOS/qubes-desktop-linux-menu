#!/bin/sh
systemctl --user import-environment \
  XDG_CURRENT_DESKTOP XDG_DATA_HOME XDG_CONFIG_HOME XDG_STATE_HOME \
  XDG_DATA_DIRS XDG_CONFIG_DIRS XDG_CACHE_HOME XDG_RUNTIME_DIR \
  XDG_SESSION_CLASS XDG_SESSION_DESKTOP XDG_SESSION_ID XDG_SESSION_PATH \
  XDG_SESSION_TYPE
systemctl --user start qubes-app-menu
